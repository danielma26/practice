redvelvet =['irene','yeri','seulgi','wendy','joy']

redvelvet1 ="redder"

def reverse (s) :
    return s[::-1]

def isPalindrome (s):
    rev = reverse(s)
    if (s==rev):
        return True
    else :
        return False


s= redvelvet1
ans = isPalindrome(s)

if ans ==1:
    print ("Yes")
else:
    print ("no")

r= len(redvelvet1)
print ("length of redvelvet: %s" %r)


def Fibonacci(n):
    if n<0:
        print("Incorrect input")
    # First Fibonacci number is 0
    elif n==1:
        return 0
    # Second Fibonacci number is 1
    elif n==2:
        return 1
    else:
        return Fibonacci(n-1)+Fibonacci(n-2)

print (Fibonacci(10))
